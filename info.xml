    <url desc="Documentation">https://github.com/davidjosephhayes/CiviCRM-Default-Mail-Tokens</url>
    <url desc="Support">https://github.com/davidjosephhayes/CiviCRM-Default-Mail-Tokens</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2015-07-22</releaseDate>
  <version>1.0</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>4.2</ver>
  </compatibility>
  <comments></comments>
  <civix>
    <namespace>CRM/Defaultmailtokens</namespace>
  </civix>
</extension>
